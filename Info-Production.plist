<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Information -->
    <key>CFBundleDisplayName</key>
    <string>Seline</string>
    <key>CFBundleIdentifier</key>
    <string>com.vibecode.seline</string>
    <key>CFBundleName</key>
    <string>Seline</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>
    
    <!-- Privacy Usage Descriptions -->
    <key>NSCameraUsageDescription</key>
    <string>Seline needs camera access to scan and attach documents to your emails.</string>
    
    <key>NSContactsUsageDescription</key>
    <string>Seline needs contacts access to help you compose emails to people in your address book.</string>
    
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>Seline uses your location to provide relevant weather and local information in search results.</string>
    
    <key>NSMicrophoneUsageDescription</key>
    <string>Seline needs microphone access to record voice notes and audio attachments for your emails.</string>
    
    <key>NSPhotoLibraryUsageDescription</key>
    <string>Seline needs photo library access to attach images and documents to your emails.</string>
    
    <key>NSAppleEventsUsageDescription</key>
    <string>Seline needs Apple Events access to integrate with other productivity apps on your device.</string>
    
    <!-- Network and External Services -->
    <key>NSAppTransportSecurity</key>
    <dict>
        <key>NSAllowsArbitraryLoads</key>
        <false/>
        <key>NSExceptionDomains</key>
        <dict>
            <key>googleapis.com</key>
            <dict>
                <key>NSExceptionRequiresForwardSecrecy</key>
                <false/>
                <key>NSIncludesSubdomains</key>
                <true/>
            </dict>
            <key>openai.com</key>
            <dict>
                <key>NSExceptionRequiresForwardSecrecy</key>
                <false/>
                <key>NSIncludesSubdomains</key>
                <true/>
            </dict>
        </dict>
    </dict>
    
    <!-- URL Schemes for OAuth -->
    <key>CFBundleURLTypes</key>
    <array>
        <dict>
            <key>CFBundleURLName</key>
            <string>Google OAuth</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>com-vibecode-seline</string>
            </array>
        </dict>
    </array>
    
    <!-- Background Modes -->
    <key>UIBackgroundModes</key>
    <array>
        <string>background-fetch</string>
        <string>background-processing</string>
    </array>
    
    <!-- App Store Requirements -->
    <key>ITSAppUsesNonExemptEncryption</key>
    <false/>
    
    <key>NSSupportsAutomaticTermination</key>
    <true/>
    
    <key>NSSupportsSuddenTermination</key>
    <true/>
    
    <!-- Launch Screen -->
    <key>UILaunchScreen</key>
    <dict>
        <key>UIColorName</key>
        <string>LaunchScreenBackground</string>
        <key>UIImageName</key>
        <string>LaunchIcon</string>
        <key>UIImageRespectsSafeAreaInsets</key>
        <true/>
    </dict>
    
    <!-- Scene Configuration -->
    <key>UIApplicationSceneManifest</key>
    <dict>
        <key>UIApplicationSupportsMultipleScenes</key>
        <false/>
        <key>UISceneConfigurations</key>
        <dict>
            <key>UIWindowSceneSessionRoleApplication</key>
            <array>
                <dict>
                    <key>UISceneConfigurationName</key>
                    <string>Default Configuration</string>
                    <key>UISceneDelegateClassName</key>
                    <string>$(PRODUCT_MODULE_NAME).SceneDelegate</string>
                </dict>
            </array>
        </dict>
    </dict>
    
    <!-- Supported Interface Orientations -->
    <key>UISupportedInterfaceOrientations</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string>
        <string>UIInterfaceOrientationPortraitUpsideDown</string>
    </array>
    
    <key>UISupportedInterfaceOrientations~ipad</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string>
        <string>UIInterfaceOrientationPortraitUpsideDown</string>
        <string>UIInterfaceOrientationLandscapeLeft</string>
        <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    
    <!-- Status Bar -->
    <key>UIStatusBarHidden</key>
    <false/>
    
    <key>UIStatusBarStyle</key>
    <string>UIStatusBarStyleDefault</string>
    
    <!-- Launch Options -->
    <key>UIRequiresFullScreen</key>
    <false/>
    
    <!-- Capabilities -->
    <key>UIRequiredDeviceCapabilities</key>
    <array>
        <string>armv7</string>
    </array>
    
    <!-- Minimum OS Version -->
    <key>LSMinimumSystemVersion</key>
    <string>16.0</string>
    
    <!-- App Category -->
    <key>LSApplicationCategoryType</key>
    <string>public.app-category.productivity</string>
    
    <!-- Data Protection -->
    <key>NSDataDetectorsDataDomains</key>
    <array>
        <string>NSDataDetectorsDomainPhoneNumber</string>
        <string>NSDataDetectorsDomainHttpURL</string>
        <string>NSDataDetectorsDomainDate</string>
        <string>NSDataDetectorsDomainAddress</string>
    </array>
    
    <!-- File Sharing -->
    <key>LSSupportsOpeningDocumentsInPlace</key>
    <true/>
    
    <key>UIFileSharingEnabled</key>
    <false/>
    
    <!-- Handoff -->
    <key>NSUserActivityTypes</key>
    <array>
        <string>com.vibecode.seline.compose-email</string>
        <string>com.vibecode.seline.search-emails</string>
    </array>
    
    <!-- Widget Support -->
    <key>NSExtension</key>
    <dict>
        <key>NSExtensionPointIdentifier</key>
        <string>com.apple.widgetkit-extension</string>
    </dict>
    
    <!-- Accessibility -->
    <key>UIAccessibilityTraits</key>
    <string>UIAccessibilityTraitAllowsDirectInteraction</string>
    
    <!-- Localization -->
    <key>CFBundleDevelopmentRegion</key>
    <string>en</string>
    
    <key>CFBundleLocalizations</key>
    <array>
        <string>en</string>
        <string>es</string>
        <string>fr</string>
        <string>de</string>
        <string>it</string>
        <string>pt</string>
        <string>ja</string>
        <string>ko</string>
        <string>zh-Hans</string>
        <string>zh-Hant</string>
    </array>
    
    <!-- Keychain Sharing -->
    <key>keychain-access-groups</key>
    <array>
        <string>$(AppIdentifierPrefix)com.vibecode.seline</string>
    </array>
</dict>
</plist>